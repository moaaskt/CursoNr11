<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#035106"
    tools:context=".MainActivity5">

    <Button
        android:id="@+id/button6"
        android:layout_width="106dp"
        android:layout_height="67dp"
        android:backgroundTint="#151313"
        android:onClick="avancar"
        android:text="Avançar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup4"
        app:layout_constraintVertical_bias="0.517" />

    <RadioGroup
        android:id="@+id/radioGroup4"
        android:layout_width="286dp"
        android:layout_height="195dp"
        android:layout_marginTop="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13">

        <RadioButton
            android:id="@+id/radioButton13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#17F3EBEB"
            android:buttonTint="@color/black"
            android:text="veículos de transporte de carga;"
            android:textColor="#EFE9E9"
            android:textSize="16sp"
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/radioButton14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#310C0C0C"
            android:buttonTint="#090909"
            android:text="caminhão ;"
            android:textColor="#F3EEEE"
            android:textSize="16sp"
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/radioButton15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#1DFAF6F6"
            android:buttonTint="#121111"
            android:text="paleteira;"
            android:textColor="#F4F0F0"
            android:textSize="16sp"
            android:textStyle="italic" />

        <RadioButton
            android:id="@+id/radioButton16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#281C1B1B"
            android:buttonTint="#151414"
            android:text="empilhadeira"
            android:textColor="#F6F2F2"
            android:textSize="16sp"
            android:textStyle="italic" />
    </RadioGroup>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="291dp"
        android:layout_height="92dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="45dp"
        android:lineSpacingExtra="2sp"
        android:text="4)Veículo autopropulsor com três rodas, pelo menos, projetado para levantar, transportar eposicionar materiais, define?"
        android:textColor="#F3F1F1"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/radioGroup4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>